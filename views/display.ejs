<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=title%></title>
    <link rel="stylesheet" href="/css/home.css">
</head>

<body>
    <input type="text" id="search-input" onkeyup="searchData()" placeholder="search">
    <table>
        <div>
        <% for(let i of Object.keys(results[0])){ %> 
        <tr id= "header-row">
          <th> <%= i %>  </th>
         
        </tr>
        <% } %>
       
        </div>
        
        <br />
        
        <div id="display-data">
        <% for(let l of results){ %> 
            <div>
            <tr id="body-row">
             <% for(let j in l){ %> 
              <td> <%= l[j] %>  </td>
              <% } %>
            </tr>
            </div>
            <% } %>
        </div>   
    </table>
<script src="/js/display.js"></script>
</body>
</html>